<body class="white-bg">
    <div class="wrapper wrapper-content animated fadeInRight ibox-content">
        <form class="form-horizontal m" id="form-fznb-add">
            <h2 class="col-sm-12" style="text-align: center;font-weight: bold ">
                年报信息
            </h2>
            <h4 class="form-header h4">基本信息</h4>
            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">编号：</label>
                        <div class="col-sm-8">
                            <input name="number" class="form-control" type="text" required>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group" style="margin-top: 10px ">
                        <label class="col-sm-3 control-label">年度：</label>
                        <div class="input-group date">
                            <div class="col-sm-8">
                                <input name="year" class="form-control" placeholder="yyyy" type="text" style="display:block;float:left;width:421.33px " required>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">省：</label>
                        <div class="col-sm-8">
                            <!--<input name="province" class="form-control" type="text" required>-->
                            <select id="province" style="width: 420px;height: 34px;border:1px solid #e5e6e7" name="province" onchange="getCities1()" ></select>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">市：</label>
                        <div class="col-sm-8">
                            <!--<input name="city" class="form-control" type="text" required>-->
                            <select
                                    id="city" name="city" style="width: 420px;height: 34px;border:1px solid #e5e6e7" onchange="getAreas1()" >
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">县：</label>
                        <div class="col-sm-8">
                            <!-- <input name="county" class="form-control" type="text" required>-->
                            <select id="county" name="county" style="width: 420px;height: 34px;border:1px solid #e5e6e7" onchange="getlngLat()" >
                            </select>
                        </div>
                    </div>
                </div>

