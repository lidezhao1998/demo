<!DOCTYPE html>
<html lang="zh" xmlns:th="http://www.thymeleaf.org" >
<head>
    <th:block th:include="include :: header('详情【标点】')" />
</head>
<body class="white-bg">
<div class="wrapper wrapper-content animated fadeInRight ibox-content">
    <form class="form-horizontal m" id="form-station-add" th:object="${mobileDevice}">
        <div class="form-group">
            <label class="col-sm-3 control-label">站点名称：</label>
            <div class="col-sm-8">
                <input name="monitoringName" readonly="readonly" class="form-control" th:field="*{monitoringName}" type="text"  >
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">站点编码：</label>
            <div class="col-sm-8">
                <input name="monitoringCode" readonly="readonly" th:field="*{monitoringCode}" class="form-control" type="text" >
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">地区：</label>
            <div class="col-sm-8">
                <input name="region" readonly="readonly" class="form-control" type="text" th:field="*{region}" >
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">负责人：</label>
            <div class="col-sm-8">
                <input name="thoseResponsible" readonly="readonly" th:field="*{thoseResponsible}" class="form-control" type="text" >
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">手机号：</label>
            <div class="col-sm-8">
                <input name="phone" readonly="readonly" class="form-control" th:field="*{phone}" type="text"  >
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">经度：</label>
            <div class="col-sm-8">
                <input name="lon" readonly="readonly" class="form-control" type="text" th:field="*{lon}" th:value="${lon}" >
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">纬度：</label>
            <div class="col-sm-8">
                <input name="lat" readonly="readonly" class="form-control" type="text" th:field="*{lat}" th:value="${lat}">
            </div>
        </div>
    </form>
</div>
<th:block th:include="include :: footer" />
<script type="text/javascript">
    var prefix = ctx + "extinterface/mobileDeviceData";
    $("#form-station-add").validate({
        focusCleanup: true
    });

    function submitHandler() {
        if ($.validate.form()) {
            $.operate.save(prefix + "/addSave", $('#form-station-add').serialize());
        }
    }
</script>
</body>
</html>