    <select id="selectEvalTianbaoConstructionList" parameterType="EvalTianbaoConstruction" resultMap="EvalTianbaoConstructionResult">
        <include refid="selectEvalTianbaoConstructionVo"/>
        <where>  
            <if test="annual != null  and annual != ''"> and ANNUAL = #{annual}</if>
            <if test="area != null  and area != ''"> and AREA = #{area}</if>
            <if test="afforestionAreaTotal != null "> and AFFORESTION_AREA_TOTAL = #{afforestionAreaTotal}</if>
            <if test="artificialAfforestionTotal != null "> and ARTIFICIAL_AFFORESTION_TOTAL = #{artificialAfforestionTotal}</if>
