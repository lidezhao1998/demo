            <if test="manageAreaTotal != null "> and MANAGE_AREA_TOTAL = #{manageAreaTotal}</if>
            <if test="nationalForest != null "> and NATIONAL_FOREST = #{nationalForest}</if>
            <if test="nationalPublicForest != null "> and NATIONAL_PUBLIC_FOREST = #{nationalPublicForest}</if>
            <if test="localPublicForest != null "> and LOCAL_PUBLIC_FOREST = #{localPublicForest}</if>
