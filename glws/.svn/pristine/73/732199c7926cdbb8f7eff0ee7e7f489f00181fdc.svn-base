    <sql id="selectEvalTianbaoConstructionVo">
        select ID, ANNUAL, AREA, AFFORESTION_AREA_TOTAL, ARTIFICIAL_AFFORESTION_TOTAL, SHRUB_AREA, AIR_SEND_AFFOREST, NEW_AFFOREST_TOTAL, NO_NEW_AFFOREST_AREA, HAVE_NEW_AFFOREST_AREA, DEGRA_REPAIR_AREA, FOREST_TEND_AREA, YEAREND_AFFOREST_AREA, MANAGE_AREA_TOTAL, NATIONAL_FOREST, NATIONAL_PUBLIC_FOREST, LOCAL_PUBLIC_FOREST from eval_tianbao_construction
