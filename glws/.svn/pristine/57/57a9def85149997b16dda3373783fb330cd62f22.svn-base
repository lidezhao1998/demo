<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.integration.mapper.DataSyncMapper">
    
    <resultMap type="DataSync" id="DataSyncResult">
    	<id     property="id"      column="id"      />
        <result property="startTime"    column="start_time"    />
        <result property="endTime"     column="end_time"     />
        <result property="status"   column="status"   />
        <result property="misfirePolicy"    column="misfire_policy"    />
        <result property="dataSource"    column="data_source"    />
        <result property="createBy"      column="create_by"      />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"      column="update_by"      />
        <result property="updateTime"    column="update_time"    />
    </resultMap>
    
    <sql id="selectDataSyncVo">
        select id,start_time,end_time,status,misfire_policy,data_source,create_by,create_time,update_by,update_time
		from a_datasync
    </sql>
    
    <!-- 查询条件 -->
	<sql id="sqlwhereSearch">
		<where>
			<if test="id !=null">
				and id = #{id}
			</if>
		</where>
	</sql>
    
    <select id="selectDataSync" parameterType="DataSync" resultMap="DataSyncResult">
        <include refid="selectDataSyncVo"/>
        <include refid="sqlwhereSearch"/>
    </select>
    
    <select id="selectDataSyncList" parameterType="DataSync" resultMap="DataSyncResult">
        <include refid="selectDataSyncVo"/>
        <where>
			<if test="id != null and id != ''">
				AND id =#{id}
			</if>
		</where>
    </select>

    <!--<delete id="deleteConfigByIds" parameterType="String">-->
        <!--delete from sys_config where config_id in -->
        <!--<foreach item="configId" collection="array" open="(" separator="," close=")">-->
        	<!--#{configId}-->
        <!--</foreach>-->
    <!--</delete>-->
    
</mapper>