        $("#bootstrap-table").append(" <tbody><tr >\n" +
            "                        <td class=\"tg-lboi\">\n" +
            "                            <input name=\"cPlotList[" + number + "].samplingArea\" class=\"form-control\" type=\"text\">\n" +
            "                        </td>\n" +
