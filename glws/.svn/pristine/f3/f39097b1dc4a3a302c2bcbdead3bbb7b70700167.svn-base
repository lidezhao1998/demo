            content: url,
            btn: ['确定', '关闭'],
            yes: function (index, layero) {
                var iframeWin = layero.find('iframe')[0];
                var code = iframeWin.contentWindow.chuanzhi();
                if (code == false) {

                } else {
                    addTab5(code, type, typetext, samplingArea, samplingAreatext);

