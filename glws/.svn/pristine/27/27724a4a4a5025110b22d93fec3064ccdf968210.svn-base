                    valign: 'middle',
                    formatter: function(value, row, index) {
                        var actions = [];
                        actions.push('<a class="btn btn-info btn-xs ' + editFlag + '" href="javascript:void(0)" onclick="dhauthUser(\'' + row.id + '\')"><i class="fa fa-search"></i>详情</a>');


