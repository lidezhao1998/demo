<body class="white-bg">
    <div class="wrapper wrapper-content animated fadeInRight ibox-content">
        <form class="form-horizontal m" id="form-fznb-add">
            <h2 class="col-sm-12" style="text-align: center;font-weight: bold ">
                年报信息
            </h2>
            <h4 class="form-header h4">基本信息</h4>
            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label"><span style="color: red; ">*</span>编号：</label>
                        <div class="col-sm-8">
                            <input id="number" name="number" class="form-control" readonly>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group" style="margin-top: 10px ">
                        <label class="col-sm-3 control-label"><span style="color: red; ">*</span>年度：</label>
                        <div class="input-group date">
                            <div class="col-sm-8">
                                <input name="year" id="test2" class="form-control" placeholder="yyyy" type="text" style="display:block;float:left;width:421.33px " required>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">


                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label"><span style="color: red; ">*</span>经济损失：</label>
                        <div class="col-sm-8">
                            <input name="economicLoss" class="form-control" type="text" onkeyup="value=value.replace(/[^\d^\.]+/g,'')" required  maxlength="6">
                        </div>
                    </div>
                </div>

                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">家畜死亡：</label>
                        <div class="col-sm-8">
                            <input name="livestockDeath" class="form-control" type="text" onkeyup="value=value.replace(/[^\d^\.]+/g,'')" maxlength="6">
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">


                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">其中工程区危害面积：</label>
                        <div class="col-sm-8">
                            <input name="hazardArea" class="form-control" type="text" onkeyup="value=value.replace(/[^\d^\.]+/g,'')" maxlength="6">
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">危害总面积：</label>
                        <div class="col-sm-8">
                            <input name="hazardAllArea" class="form-control" type="text" onkeyup="value=value.replace(/[^\d^\.]+/g,'')" maxlength="6">
                        </div>
                    </div>
                </div>

            </div>
            <div class="row">

                <div class="col-sm-6">
                    <div class="form-group">
