                        <div class="col-sm-8">
                            <input name="insectharmCounty" class="form-control" type="text" onkeyup="value=value.replace(/[^\d^\.]+/g,'')" maxlength="6">
                        </div>
                    </div>
                </div>
            </div>

        </form>
        <div class="button-s" style="margin-left: 70%">
