/* 功能图标 */
.header-r a::before {
    position: absolute;
    top: 7px;
    left: 0;
    width: 18px;
    height: 18px;
    content: "";
}
/* 条件查询 */
.header-r a#conditionQuery::before {
    background: url(./../../img/icon-query.png) no-repeat center;
}
/* 定位 */
.header-r a#locationShow::before {
    background: url(./../../img/zs-map-tool-dw-g.png) no-repeat center;
}
/* 点查 */
.header-r a#pointSerch::before {
    background: url(./../../img/icon-point.png) no-repeat center;
}
/* 标记列表 */
.header-r a#markedList::before {
    background: url(./../../img/icon-mark-list-s.png) no-repeat center;
}
/* 标记面 */
.header-r a#markingSurface::before {
    background: url(./../../img/icon-mark-m-s.png) no-repeat center;
}
/* 标记点 */
.header-r a#markPoint::before {
    background: url(./../../img/icon-mark-d-s.png) no-repeat center;
}
/* 测面 */
.header-r a#measuringSurface::before {
    background: url(./../../img/tool-face-cl.png) no-repeat center;
}
/* 测距 */
.header-r a#ranging::before {
    background: url(./../../img/tool-box-cl.png) no-repeat center;
}
/* 比对 */
.header-r a#comparison::before {
    background: url(./../../img/head-tool-fp.png) no-repeat center;
}
/* 退出 */
.header-r a#gisQuit::before {
    background: url(./../../img/head-tool-fptc.png) no-repeat center;
}
/* 3d */
.header-r a#show3d::before {
    background: url(./../../img/icon-23d.png) no-repeat center;
}
/* 卷帘 */
.header-r a#jl::before {
    background: url(./../../img/tool-box-jl.png) no-repeat center;
}
