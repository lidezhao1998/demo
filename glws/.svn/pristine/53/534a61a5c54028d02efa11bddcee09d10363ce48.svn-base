}

input[type=range]{
    outline: none;
    -webkit-appearance: none;
    width:100% !important;
    background-color:transparent;
    height: 1px;
}
input[type=range]::-webkit-slider-thumb {
    opacity: 0.7;
    -webkit-appearance: none;
    height:40px;
    width: 8px;
    background: none repeat scroll 0 0 #ffffff;
    border-radius: 10px;
}
