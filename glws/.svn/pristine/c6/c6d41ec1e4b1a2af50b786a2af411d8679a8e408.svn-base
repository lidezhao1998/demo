<!DOCTYPE html>
<html lang="zh" xmlns:th="http://www.thymeleaf.org" >
<head>
    <th:block th:include="include :: header('新增物资')" />
</head>
<body class="white-bg">
    <div class="wrapper wrapper-content animated fadeInRight ibox-content">
        <form class="form-horizontal m" id="form-manufactor-add">

                    <div class="form-group">
                        <label class="col-sm-3 control-label">物资编号：</label>
                        <div class="col-sm-8">
                            <input name="code" class="form-control" type="text">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">物资名称：</label>
                        <div class="col-sm-8">
                            <input name="materialName" class="form-control" type="text">
                        </div>
                    </div>

            <div class="form-group">
                <label class="col-sm-3 control-label">字典类型：</label>
                <div class="col-sm-8">
                    <input class="form-control" type="text" id="materialType" name="materialType" readonly="true" th:value="${dictType}">
                </div>
            </div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label">厂家：</label>
                        <div class="col-sm-8">
                            <input name="manufactor" class="form-control" type="text">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-sm-3 control-label">物资描述：</label>
                        <div class="col-sm-8">
                            <input name="remark" class="form-control" type="text">
                        </div>
                    </div>


        </form>
    </div>
    <th:block th:include="include :: footer" />
    <script type="text/javascript">
        var prefix = ctx + "reserves/manufactor";
        $("#form-manufactor-add").validate({
            focusCleanup: true
        });

        function submitHandler() {
            if ($.validate.form()) {
                $.operate.save(prefix + "/add", $('#form-manufactor-add').serialize());
            }
        }
    </script>
</body>
</html>