<!DOCTYPE html>
<html lang="zh" xmlns:th="http://www.thymeleaf.org" >
<head>
    <th:block th:include="include :: header('查看Gis信息')" />
</head>
<body class="white-bg">
<div class="wrapper wrapper-content animated fadeInRight ibox-content">
    <form class="form-horizontal m" id="form-config-add">
        <div class="form-group">
            <label class="col-sm-3 control-label">绘制标题：</label>
            <div class="col-sm-8">
                <label class="form-control">[[${gisMap.name}]]</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">绘制类型：
                <label th:if="${gisMap.type}==1">
                    标点
                </label>
                <label th:if="${gisMap.type}==2">
                    图层
                </label>
            </label>

        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">专题类型：</label>
            <div class="col-sm-8">
                <select name="project_type" th:with="type=${@dict.getGisFeatures()}" class="form-control" disabled="disabled">
                    <option th:each="dict : ${type}" th:text="${dict.name}" th:value="${dict.id}" th:selected="${gisMap.project_type} == ${dict.id} ? true :false"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">危害等级：</label>
            <div class="col-sm-8">
                <select name="hazard_rating" th:with="type=${@dict.getType('sys_hazard_rating')}" class="form-control" disabled="disabled">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:selected="${gisMap.hazard_rating} == ${dict.dictValue} ? true :false"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">所占面积(km²)：</label>
            <div class="col-sm-8">
                <label class="form-control">[[${gisMap.area}]]</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">所在地区：</label>
            <div class="col-sm-8">
                <label  class="form-control">[[${gisMap.address}]]</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">备注详情：</label>
            <div class="col-sm-8">
                <label class="form-control">[[${gisMap.remark}]]</label>
            </div>
        </div>
    </form>
</div>
<th:block th:include="include :: footer" />
<script th:inline="javascript">
    var prefix = ctx + "system/gisMap"
    $("#form-config-add").validate({
        focusCleanup: true
    });
</script>
</body>
</html>