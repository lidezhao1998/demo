package com.ruoyi.zaihai.enums;

public enum IdeaType {

    /**
     * 保存
     */
    IDEA_WEI("1","保存"),
    /**
     * 保存并提交意见
     */
    IDEA_YI("2","提交"),
    /**
     * 删除意见
     */
    IDEA_DELE("3","删除");

    private final String status;

    private final String name;

    IdeaType(String status, String name) {
        this.status = status;
        this.name = name;
    }

    public String getStatus() {
        return status;
    }

    public String getName() {
        return name;
    }
}