            let current = $('input[name="'+list[i]+'"]');
            let sum = 0;
            for(var y = 0; y < current.length; y++){
                if(current[y].value!=''&&current[y].value!=null){
                    sum += parseInt(current[y].value);
                }
            }
            $('#' + list[i]).val(sum);
