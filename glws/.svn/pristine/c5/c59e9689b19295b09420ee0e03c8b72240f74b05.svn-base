<script th:inline="javascript">
    var prefix = ctx + "system/publish";

    $(function () {
        var options = {
            url: prefix + "/list",
            importUrl: prefix + "/importData",
            importTemplateUrl: prefix + "/importTemplate",
            showSearch: false,
            showToggle: false,
            showColumns: false,
            showRefresh: false,
            pagination: false
        };
        $.table.init(options);
    });
</script>
<script type="text/javascript">
    $("#form-publish-add").validate({
        rules: {
            onkeyup: false,
            min: true,
        },
        errorPlacement: function (error, element) {
            $.modal.alert('请输入不小于0的数字！');
        },
        focusCleanup: true
    });


    function save() {
        $.post(prefix + "/publishList", $('#form-publish-add').serialize(), function (data) {
            //data中是服务端返回的数据
            //这里进行成功后的渲染。。。。。。

        });
        return false;//在这里返回false
    }


    layui.use('laydate', function () {
        var laydate = layui.laydate;

        //执行一个laydate实例
        laydate.render({
            elem: '#test2',
            type: 'year',
            min: 'new Date()'
        });
    });

    function btnyizhu() {
        if ($.validate.form()) {
            let flag = true;
            $('.pulish input[type=text]').each(function () {
                var name = $(this).val();
                if (name) {
                    flag = false;
                    return false;
                }
            });
            if (flag) {
                $.modal.confirm("信息未填写成,请填写后提交", function () {
                })
            } else if ($("#test2").val() == '') {
                $.modal.confirm("年份未填写,请填写后提交", function () {
                })
            } else {
                var orderItemArray = new Array();

                $("#form-publish-add tr").each(function () {
                    //this代表当前dom对象tr
                    var that = this;
                    var orderItemObj = new Object();
                    $(that).find("select,input").each(function () {
                        var name = $(this).attr("name");
                        orderItemObj[name] = $(this).val();
                    });
                    orderItemObj['year'] = $("#test2").val();
                    orderItemArray.push(orderItemObj);
                });
                var url = ctx + "system/publish/addFaBu";
                $.ajax({
                    url: url,
                    dataType: "json",
                    contentType: "application/json;charsetset=UTF-8",//必须
                    type: "post",
                    cache: false,
                    data: JSON.stringify(orderItemArray),//必须
                    success: function (obj) {
                        if (obj == "400") {
                            layer.alert('任务发布成功！', function (index) {
                                $.modal.close();
                                parent.location.reload();
                            });
                        } else {
                            layer.alert("发布失败，请查看网络！！！");
                        }
                    },
                    error: function (textStatus, e) {
                        layer.alert("系统ajax交互错误: ");
                    }
                });
            }
        }
    }

    function publishSave() {
        if ($.validate.form()) {
            let flag = false;
            /*$('.pulish input[type=text]').each(function () {
                var name = $(this).val();
                if (name != '') {
                    flag = false;
                }
            });*/
            if (flag) {
                $.modal.confirm("信息未填写成,请填写后提交", function () {
                })
            }
            if ($("#test2").val() == '') {
                $.modal.confirm("年份未填写,请填写后提交", function () {
                })
                return false;
            } else {
                var orderItemArray = new Array();

                $("#form-publish-add tr").each(function () {
                    //this代表当前dom对象tr
                    var that = this;
                    var orderItemObj = new Object();
                    $(that).find("select,input").each(function () {
                        var name = $(this).attr("name");
                        orderItemObj[name] = $(this).val();
                    });
                    orderItemObj['year'] = $("#test2").val();
                    orderItemArray.push(orderItemObj);
                });
                var url = ctx + "system/publish/publishSave";
                $.ajax({
                    url: url,
                    dataType: "json",
                    contentType: "application/json;charsetset=UTF-8",//必须
                    type: "post",
                    cache: false,
                    data: JSON.stringify(orderItemArray),//必须
                    success: function (obj) {
                        if (obj == "400") {
                            layer.alert('任务保存成功！', function (index) {
                                window.location.reload();
                                $.modal.close()
                            });
                        } else {
                            layer.alert("保存失败，请查看网络！！！");
                        }
                    },
                    error: function (textStatus, e) {
                        layer.alert("系统ajax交互错误: ");
                    }
                });
            }
        }
    }

</script>
<script>
    var searchinput = $("input");
    var textWidth = function (text) {
        var sensor = $('<pre>' + text + '</pre>').css({display: 'none'});
        $('body').append(sensor);
        var width = sensor.width();
        sensor.remove();
        return width;
    };
    searchinput.unbind('keyup').bind('keyup', function () {
        $(this).width(textWidth($(this).val() + 20));
    });
</script>
