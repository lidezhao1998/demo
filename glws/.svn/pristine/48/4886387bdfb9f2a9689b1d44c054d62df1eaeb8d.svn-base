                                actions.push('<a class="btn btn-danger btn-xs ' + normalInspectionRemoveFlag + '" href="javascript:void(0)" onclick="$.operate.remove(\'' + row.id + '\')"><i class="fa fa-remove"></i>删除</a>');
                                actions.push('<a class="btn btn-warning btn-xs ' + normalInspectionEditFlag + '" href="javascript:void(0)" onclick="auditnormalInspection(\'' + row.id + '\')"><i class="fa fa-search"></i>审核</a>');
                            }else{
                                actions.push('<a class="btn btn-warning btn-xs ' + normalInspectionEditFlag + '" href="javascript:void(0)" onclick="unauditnormalInspection(\'' + row.id + '\')"><i class="fa fa-search"></i>取消审核</a>');
