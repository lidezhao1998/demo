    });

    function onLoad() {
        load();
        if(lngLat!=null){
            for(var i=0;i<lngLat.length;i++){
                var safdy=lngLat [i].latitude;
                if(safdy!=null && safdy!=''){
                    drawPoint1(safdy);
                }
                if(gid!=null){
                    loadgisByGid(gid);
                }
            }
        }
    }

