<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.integration.mapper.AuditMapper">
    
    <resultMap type="Audit" id="AuditResult">
    	<id     property="id"      column="id"      />
        <result property="auditUser"    column="audit_user"    />
        <result property="auditOrg"     column="audit_org"     />
        <result property="auditResult"   column="audit_result"   />
        <result property="auditContent"    column="audit_content"    />
        <result property="auditDate"    column="audit_date"    />
        <result property="createBy"      column="create_by"      />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"      column="update_by"      />
        <result property="updateTime"    column="update_time"    />
        <result property="normalInspectionId"    column="normal_inspection_id"    />

    </resultMap>
    
    <sql id="selectAuditVo">
        select id,audit_user,audit_org,audit_result,audit_content,auit_data,create_by,create_time,update_by,update_time,normal_inspection_id
		from a_audit
    </sql>
    
    <!-- 查询条件 -->
	<sql id="sqlwhereSearch">
		<where>
			<if test="id !=null">
				and id = #{id}
			</if>
            <if test="normalInspectionId !=null">
                and normal_inspection_id = #{normalInspectionId}
            </if>
		</where>
	</sql>
    
    <select id="selectAudit" parameterType="Audit" resultMap="AuditResult">
        <include refid="selectAuditVo"/>
        <include refid="sqlwhereSearch"/>
    </select>
    
    <select id="selectAuditList" parameterType="Audit" resultMap="AuditResult">
        <include refid="selectAuditVo"/>
        <where>
			<if test="id != null and id != ''">
				AND id =#{id}
			</if>
            <if test="normalInspectionId !=null">
                and normal_inspection_id = #{normalInspectionId}
            </if>
		</where>
    </select>

    <!--<delete id="deleteConfigByIds" parameterType="String">-->
        <!--delete from sys_config where config_id in -->
        <!--<foreach item="configId" collection="array" open="(" separator="," close=")">-->
        	<!--#{configId}-->
        <!--</foreach>-->
    <!--</delete>-->
    
</mapper>