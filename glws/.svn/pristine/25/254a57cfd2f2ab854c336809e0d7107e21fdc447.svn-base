package com.sinosoft.system.domain;

import com.ruoyi.common.core.domain.BaseEntity;

import java.awt.*;

public class MapOptions extends BaseEntity {
    //地址
    private String prefix;

    //地图投影类型信息
    private String getCode;

    //地理坐标
    private String getMapBounds;

    //地图当前中心点
    private  String getCenter;

    //地图大小
    private Point getSize;

    //地图缩放级别
    private String getZoom;

    //两点之间的距离
    private String getDistance;

    //根据提供的地理区域或坐标获得最佳的地图视野
    private String getViewport;

    //地图拖拽功能
    public boolean enableDrage(){return true;}

    public boolean disableDrag(){return false;}

    //滚轮放大缩小功能
    public boolean enableScrollWheelZoom(){return true ;}

    public boolean disableScrollWheelZoom(){return false;}


    //双击放大缩小功能
    public boolean enableDoubleClickZoom(){return true;}

    public boolean disableDoubleClickZoom(){return false;}

    //启用键盘操作
    public boolean enableKeyboard(){return true;}

    public boolean disableKeyboard(){return false;}

    //地图惯性拖拽
    public boolean enableInertia(){return true;}

    public boolean disableInertia(){return false;}

    //连续缩放效果

    public boolean enablContinuousZoom(){return true;}

    public boolean disableContinuousZoom(){return false;}







}
