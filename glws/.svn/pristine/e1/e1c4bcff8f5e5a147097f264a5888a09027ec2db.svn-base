                $.modal.confirm("信息未填写成,请填写后提交", function () {})
            } else {
                $.modal.confirm("确认分解任务吗？", function () {
                    $.operate.save(prefix + "/add", $('#form-resolve-add').serialize());
                })
            }
        }
