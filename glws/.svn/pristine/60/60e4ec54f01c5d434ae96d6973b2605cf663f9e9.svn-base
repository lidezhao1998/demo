            // $("#type3").val(res.city.cityName);
            // plotCity(res.city.cityName, "name='" + res.city.cityName + "'", res.city.id, "qu");

        })
    }

    function showDiv() {
        $("#videoSelect").hide();
        $("#videoDiv").hide();
        $("#map").show();
        //显示列表
        $("#initTable").show();

    }

