                            <li>
                                <a class="btn btn-primary btn-rounded btn-sm"
                                   onclick="$.table.search('dept-form','slsj')"><i class="fa fa-search"></i>&nbsp;搜索</a>
                                <a class="btn btn-warning btn-rounded btn-sm"
                                   onclick="$.form.reset('dept-form','slsj')"><i class="fa fa-refresh"></i>&nbsp;重置</a>

                                   <!-- <a href="javascript:;" class="btn  btn-rounded btn-sm"
                                       style="margin-top: -8px;margin-bottom: -3px;background-color: #1c84c6;color: #FFFFFF;" shiro:hasPermission="extinterface:landsurveyList:import" onclick="drawGis('Point','mobileDeviceTab')">新增</a>
-->
                            </li>
                        </ul>
                    </div>
                </form>
            </div>
            <div class="col-sm-12 select-table table-striped" style="overflow-x: scroll">
                <table id="slsj" style="width: 100%"></table>
            </div>
        </div>
    </div>
</div>
<div id="videoDiv" border="10px" style="width: 100%;height: 100%">
</div>
<div id="videoSelect">
    <div style="    top: 20px;
    position: absolute;
    left: 20px;
    background-color: white;
    color: black;">

