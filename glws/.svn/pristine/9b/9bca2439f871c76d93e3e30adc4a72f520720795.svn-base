    <select id="selectCTaskTechnologyList" parameterType="CTaskTechnology" resultMap="CTaskTechnologyResult">
        <include refid="selectCTaskTechnologyVo"/>
        <where>
            <if test="sumbitter != null  and sumbitter != ''"> and SUMBITTER = #{sumbitter}</if>
            <if test="name != null  and name != ''">and NAME like concat('%', #{name}, '%')</if>
            <if test="technicalField != null  and technicalField != ''"> and TECHNICAL_FIELD = #{technicalField}</if>
            <if test="email != null  and email != ''"> and EMAIL = #{email}</if>
