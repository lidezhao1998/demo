            <if test="phone != null "> and PHONE = #{phone}</if>
            <if test="fax != null  and fax != ''"> and FAX = #{fax}</if>
            <if test="technologyMaturity != null  and technologyMaturity != ''"> and TECHNOLOGY_MATURITY = #{technologyMaturity}</if>
            <if test="rdmethods != null  and rdmethods != ''"> and RDMETHODS = #{rdmethods}</if>
            <if test="cooperationMethod != null  and cooperationMethod != ''"> and COOPERATION_METHOD = #{cooperationMethod}</if>
            <if test="resultsApplication != null  and resultsApplication != ''"> and RESULTS_APPLICATION = #{resultsApplication}</if>
            <if test="demand != null  and demand != ''"> and DEMAND = #{demand}</if>
            <if test="industry != null  and industry != ''"> and INDUSTRY = #{industry}</if>
            <if test="awardCategory != null  and awardCategory != ''"> and AWARD_CATEGORY = #{awardCategory}</if>
            <if test="awardLevel != null  and awardLevel != ''"> and AWARD_LEVEL = #{awardLevel}</if>
            <if test="creatTime != null "> and CREAT_TIME = #{creatTime}</if>
            <if test="reportTime != null "> and REPORT_TIME = #{reportTime}</if>
            <if test="inputTime != null "> and INPUT_TIME = #{inputTime}</if>
            <if test="createBy != null  and createBy != ''"> and CREATE_BY = #{createBy}</if>
            <if test="status != null  and status != ''"> and STATUS = #{status}</if>
        </where>
        order by CREAT_TIME desc

    </select>

    <select id="selectCTaskTechnologyById" parameterType="Long" resultMap="CTaskTechnologyResult">
        <include refid="selectCTaskTechnologyVo"/>
        where ID = #{id}
    </select>

