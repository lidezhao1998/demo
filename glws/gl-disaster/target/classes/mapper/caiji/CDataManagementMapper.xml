    </update>

    <delete id="deleteCDataManagementById" parameterType="Long">
        delete from c_data_management where ID = #{id}
    </delete>

    <delete id="deleteCDataManagementByIds" parameterType="String">
        delete from c_data_management where ID in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

