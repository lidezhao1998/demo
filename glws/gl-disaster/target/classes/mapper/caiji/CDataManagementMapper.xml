            <if test="registeredAddress != null  and registeredAddress != ''">REGISTERED_ADDRESS,</if>
            <if test="businessScope != null  and businessScope != ''">BUSINESS_SCOPE,</if>
            <if test="remarks != null  and remarks != ''">REMARKS,</if>
            <if test="createBy != null  and createBy != ''">CREATE_BY,</if>
            <if test="createTime != null ">CREATE_TIME,</if>
            <if test="updateBy != null  and updateBy != ''">UPDATE_BY,</if>
            <if test="updateTime != null ">UPDATE_TIME,</if>
            <if test="reportTime != null ">REPORT_TIME,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null  and name != ''">#{name},</if>
            <if test="time != null ">#{time},</if>
            <if test="peopleNumber != null ">#{peopleNumber},</if>
            <if test="registeredCapital != null ">#{registeredCapital},</if>
            <if test="personName != null  and personName != ''">#{personName},</if>
            <if test="controlPlace != null  and controlPlace != ''">#{controlPlace},</if>
            <if test="controlType != null  and controlType != ''">#{controlType},</if>
            <if test="provincialLevelName != null  and provincialLevelName != ''">#{provincialLevelName},</if>
            <if test="cityLevelName != null  and cityLevelName != ''">#{cityLevelName},</if>
            <if test="countyLevelName != null  and countyLevelName != ''">#{countyLevelName},</if>
            <if test="status != null  and status != ''">#{status},</if>
            <if test="phone != null  and phone != ''">#{phone},</if>
            <if test="legalRepresentative != null  and legalRepresentative != ''">#{legalRepresentative},</if>
            <if test="userName != null  and userName != ''">#{userName},</if>
            <if test="industry != null  and industry != ''">#{industry},</if>
            <if test="unifiedCode != null  and unifiedCode != ''">#{unifiedCode},</if>
            <if test="organizationCode != null  and organizationCode != ''">#{organizationCode},</if>
            <if test="business != null  and business != ''">#{business},</if>
            <if test="taxpayer != null  and taxpayer != ''">#{taxpayer},</if>
            <if test="registrationAuthority != null  and registrationAuthority != ''">#{registrationAuthority},</if>
            <if test="administrativeDivision != null  and administrativeDivision != ''">#{administrativeDivision},</if>
            <if test="registeredAddress != null  and registeredAddress != ''">#{registeredAddress},</if>
            <if test="businessScope != null  and businessScope != ''">#{businessScope},</if>
            <if test="remarks != null  and remarks != ''">#{remarks},</if>
            <if test="createBy != null  and createBy != ''">#{createBy},</if>
            <if test="createTime != null ">#{createTime},</if>
            <if test="updateBy != null  and updateBy != ''">#{updateBy},</if>
            <if test="updateTime != null ">#{updateTime},</if>
            <if test="reportTime != null ">#{reportTime},</if>
        </trim>
    </insert>

    <update id="updateCDataManagement" parameterType="CDataManagement">
        update c_data_management
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null  and name != ''">NAME = #{name},</if>
            <if test="time != null ">TIME = #{time},</if>
            PEOPLE_NUMBER = #{peopleNumber},
            <if test="registeredCapital != null ">REGISTERED_CAPITAL = #{registeredCapital},</if>
            <if test="personName != null  and personName != ''">PERSON_NAME = #{personName},</if>
            <if test="controlPlace != null  and controlPlace != ''">CONTROL_PLACE = #{controlPlace},</if>
            <if test="controlType != null  and controlType != ''">CONTROL_TYPE = #{controlType},</if>
            <if test="status != null  and status != ''">STATUS = #{status},</if>
            PHONE = #{phone},
            PROVINCIAL_LEVEL_NAME = #{provincialLevelName},
            CITY_LEVEL_NAME = #{cityLevelName},
            COUNTY_LEVEL_NAME = #{countyLevelName},
            LEGAL_REPRESENTATIVE = #{legalRepresentative},
           USER_NAME = #{userName},
           INDUSTRY = #{industry},
            UNIFIED_CODE = #{unifiedCode},
            ORGANIZATION_CODE = #{organizationCode},
           BUSINESS = #{business},
            TAXPAYER = #{taxpayer},
            REGISTRATION_AUTHORITY = #{registrationAuthority},
           ADMINISTRATIVE_DIVISION = #{administrativeDivision},
            REGISTERED_ADDRESS = #{registeredAddress},
          BUSINESS_SCOPE = #{businessScope},
            <if test="remarks != null  and remarks != ''">REMARKS = #{remarks},</if>
            <if test="createBy != null  and createBy != ''">CREATE_BY = #{createBy},</if>
            <if test="createTime != null ">CREATE_TIME = #{createTime},</if>
